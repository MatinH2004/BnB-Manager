<div class="middle">
  <h3>New Tenant</h3>
  <form action="/new/<%= @apartment_id %>/tenant" method="post">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" value="<%= session[:name] %>"><br><br>
    <label for="rent">Rent:</label>
    <input type="text" id="rent" name="rent" value="<%= session[:rent] %>"><br><br>
    <button type="submit" value="Submit">Submit</button>
  </form>
</div>

<footer>
  <a href="/">Main Menu</a>
</footer>